{
  "name": "zodock",
  "type": "module",
  "version": "0.0.2",
  "author": "ItMaga <m-chemurziev@ya.ru>",
  "license": "MIT",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "yarn build",
    "release": "bumpp && yarn publish",
    "test": "vitest --reporter verbose",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "zod": ">=3.0.0"
  },
  "devDependencies": {
    "@antfu/eslint-config-ts": "^0.38.4",
    "@types/node": "^18.15.11",
    "bumpp": "^9.1.0",
    "date-fns": "^2.29.3",
    "eslint": "^8.37.0",
    "lint-staged": "^13.2.0",
    "simple-git-hooks": "^2.8.1",
    "typescript": "^5.0.4",
    "unbuild": "^1.2.0",
    "vite": "^4.2.1",
    "vitest": "^0.29.8",
    "zod": "^3.21.4"
  },
  "simple-git-hooks": {
    "pre-commit": "yarn lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
